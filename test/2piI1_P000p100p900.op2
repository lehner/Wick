## 2pi operator, I=1, 0 total momentum, back-to-back 100 momentum
## Gamma_5 for both pions
## assuming momenta index 0-5 correspond to ordering +x,+y,+z,-x,-y,-z
##

FACTOR 1.0
 UBAR t0 local
 GAMMA mu
 MOM p_100_type_p0
 D t0 local
 DBAR t0 local
 GAMMA mu
 MOM p_100_type_n0
 U t0 local
 HINT: 0P000A1P0[1,1,1,1,1,1] 0P000EEP0[1,1,-2,1,1,-2] 0P000EEP1[1,-1,0,1,-1,0] 0P000T1M0[1,0,0,-1,0,0] 0P000T1M1[0,1,0,0,-1,0] 0P000T1M2[0,0,1,0,0,-1]
 #HINT:
 #0P000A1P0[ 1  1  1  1  1  1]
 #0P000EEP0[ 1  1 -2  1  1 -2]
 #0P000EEP1[ 1 -1  0  1 -1  0]
 #0P000T1M0[ 1  0  0 -1  0  0]
 #0P000T1M1[ 0  1  0  0 -1  0]
 #0P000T1M2[ 0  0  1  0  0 -1]

FACTOR -1.0
 DBAR t0 local
 GAMMA mu
 MOM p_100_type_p0
 U t0 local
 UBAR t0 local
 GAMMA mu
 MOM p_100_type_n0
 D t0 local
 HINT: 0P000A1P0[1,1,1,1,1,1] 0P000EEP0[1,1,-2,1,1,-2] 0P000EEP1[1,-1,0,1,-1,0] 0P000T1M0[1,0,0,-1,0,0] 0P000T1M1[0,1,0,0,-1,0] 0P000T1M2[0,0,1,0,0,-1]

